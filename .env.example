# API Configuration
NEXT_PUBLIC_API_URL=http://api.example.com
NEXT_PUBLIC_USE_MOCK_API=false

# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key
